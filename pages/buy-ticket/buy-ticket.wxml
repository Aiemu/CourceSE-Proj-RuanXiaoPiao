<view class="place-radio">
    <van-radio-group value="{{ radio }}" bind:change="onChange" wx:for="{{placeList}}" wx:for-item="place" wx:key="">
    <van-cell-group>
        <van-cell
        title="{{place}}"
        clickable
        data-name="{{index+1}}"
        bind:click="onClick"
        >
        <van-radio slot="right-icon" name="{{index+1}}" />
        </van-cell>
    </van-cell-group>
    </van-radio-group>
</view>